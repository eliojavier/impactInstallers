{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "impact-installers"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "../src/assets/js/jquery.min.js",
        "../src/assets/js/maps-api.js",
        "../src/assets/js/jquery.ui.map.js",
        "../src/assets/js/datatables.min.js",
        "../src/assets/js/wow.min.js",
        "../src/assets/js/bootstrap.min.js",
        "../src/assets/js/jquery.superslides.min.js",
        "../src/assets/js/slick.min.js",
        "../src/assets/js/jquery.circliful.min.js",
        "../src/assets/js/modernizr.custom.js",
        "../src/assets/js/classie.js",
        "../src/assets/js/elastic_grid.min.js",
        "../src/assets/js/portfolio_slider.js",
        "../src/assets/js/custom.js",
        "../src/assets/admin/plugins/fastclick/fastclick.min.js",
        "../src/assets/admin/dist/js/app.min.js",
        "../src/assets/admin/plugins/sparkline/jquery.sparkline.min.js",
        "../src/assets/admin/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js",
        "../src/assets/admin/plugins/jvectormap/jquery-jvectormap-world-mill-en.js",
        "../src/assets/admin/plugins/slimScroll/jquery.slimscroll.min.js",
        "../node_modules/chart.js/dist/Chart.js",
        "../src/assets/admin/dist/js/pages/dashboard2.js",
        "../src/assets/admin/dist/js/demo.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
